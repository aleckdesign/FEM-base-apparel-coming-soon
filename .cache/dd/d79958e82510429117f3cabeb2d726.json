-width: 1200px) {\r\n    font-size: 70%;\r\n  }\r\n\r\n  @media only screen and (max-width: 600px) {\r\n    font-size: 55%;\r\n  }\r\n}\r\n\r\nbody {\r\n  font-size: 1.6rem;\r\n}\r\n","css\\sass\\components\\_landing.scss":"@use 'abstracts/variables' as *;\r\n\r\n.container {\r\n  min-height: 100vh;\r\n  //   margin-bottom: 6rem;\r\n  display: grid;\r\n  grid-template-columns: 1fr 30% 1fr minmax(min-content, max-content) 35%;\r\n  grid-template-rows: min-content;\r\n\r\n  @media only screen and (max-width: 1200px) {\r\n    grid-template-columns: 1fr 35% 1fr minmax(min-content, max-content) 50%;\r\n  }\r\n\r\n  @media only screen and (max-width: 800px) {\r\n    grid-template-columns: 100%;\r\n    grid-template-rows: min-content 30vh min-content 15vh;\r\n  }\r\n}\r\n\r\n.logo {\r\n  margin-top: 6vh;\r\n  grid-column: 2/3;\r\n\r\n  @media only screen and (max-width: 800px) {\r\n    grid-row: 1/2;\r\n    grid-column: 1/2;\r\n    margin-bottom: 5vh;\r\n    padding-left: 4rem;\r\n  }\r\n}\r\n\r\n.content-left {\r\n  grid-column: 2/3;\r\n  // grid-row: 2/3;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n\r\n  @media only screen and (max-width: 800px) {\r\n    grid-row: 3/4;\r\n    grid-column: 1/2;\r\n    align-items: center;\r\n    text-align: center;\r\n  }\r\n}\r\n\r\n.heading {\r\n  margin-bottom: 2.5rem;\r\n\r\n  @media only screen and (max-width: 800px) {\r\n    margin-top: 5vh;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    padding: 0 4rem;\r\n  }\r\n\r\n  &__title {\r\n    text-transform: uppercase;\r\n    font-size: 5rem;\r\n    margin-bottom: 2.5rem;\r\n    max-width: 20rem;\r\n    letter-spacing: 1.5rem;\r\n\r\n    @media only screen and (min-width: 1200px) {\r\n      font-size: 6rem;\r\n    }\r\n\r\n    @media only screen and (max-width: 800px) {\r\n      max-width: min-content;\r\n      font-size: 5rem;\r\n    }\r\n\r\n    & span {\r\n      font-weight: 300;\r\n      color: $clr-desat-red;\r\n    }\r\n  }\r\n\r\n  &__text {\r\n    color: $clr-desat-red;\r\n    line-height: 2.5rem;\r\n    max-width: 40rem;\r\n  }\r\n}\r\n\r\n.content-right {\r\n  grid-column: 5/6;\r\n  grid-row: 1 / span 2;\r\n\r\n  background-image: url(/images/hero-desktop.jpg);\r\n  background-size: cover;\r\n  background-position: top;\r\n  background-repeat: no-repeat;\r\n\r\n  @media only screen and (max-width: 800px) {\r\n    grid-row: 2/3;\r\n    grid-column: 1/2;\r\n    background-image: url(/images/hero-mobile.jpg);\r\n  }\r\n}\r\n\r\n.form {\r\n  width: 75%;\r\n  display: flex;\r\n  align-items: center;\r\n  position: relative;\r\n  max-width: 40rem;\r\n\r\n  &__input {\r\n    height: 5rem;\r\n    padding: 2rem;\r\n    width: 100%;\r\n    border-radius: 2.5rem;\r\n    border: 1px solid $clr-desat-red;\r\n    color: $clr-desat-red;\r\n\r\n    &:focus {\r\n      outline: none;\r\n      color: black;\r\n    }\r\n  }\r\n\r\n  &__btn {\r\n    border-radius: 2.5rem;\r\n    position: absolute;\r\n    height: 100%;\r\n    right: 0;\r\n    border: none;\r\n    color: transparent;\r\n    background-image: linear-gradient($grad-pink);\r\n    width: 25%;\r\n    cursor: pointer;\r\n    box-shadow: 0 1rem 2rem rgba($clr-desat-red, 0.6);\r\n\r\n    &:hover {\r\n      opacity: 0.6;\r\n    }\r\n  }\r\n\r\n  &__error-icon {\r\n    position: absolute;\r\n    right: 35%;\r\n  }\r\n\r\n  &__error-text {\r\n    position: absolute;\r\n    font-size: 1.4rem;\r\n    left: 2rem;\r\n    top: 6rem;\r\n    color: $clr-dull-red;\r\n  }\r\n\r\n  &__success {\r\n    position: absolute;\r\n    font-size: 1.4rem;\r\n    left: 2rem;\r\n    top: 6rem;\r\n    color: green;\r\n  }\r\n}\r\n\r\n.hidden {\r\n  visibility: hidden;\r\n}\r\n\r\n.invalid {\r\n  border: 2px solid $clr-dull-red;\r\n}\r\n\r\n.valid {\r\n  border: 1px solid $clr-desat-red;\r\n}\r\n\r\n.attribution {\r\n  font-size: 11px;\r\n  text-align: center;\r\n}\r\n.attribution a {\r\n  color: hsl(228, 45%, 44%);\r\n}\r\n","css\\sass\\abstracts\\_variables.scss":"\r\n\r\n$clr-desat-red: hsl(0, 36%, 70%);\r\n$clr-soft-red: hsl(0, 93%, 68%);\r\n\r\n$clr-dull-red: hsl(0, 93%, 68%);\r\n\r\n$grad-white: 135deg,hsl(0, 0%, 100%),  hsl(0, 100%, 98%);\r\n$grad-pink: 135deg,hsl(0, 80%, 86%), hsl(0, 74%, 74%);"},"lineCount":null}},"error":null,"hash":"e93f193bd22d0fd21b8f6a2b40ba0a7b","cacheData":{"env":{}}}